{
  "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths": {
          "*": ["types/*"]
      }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
